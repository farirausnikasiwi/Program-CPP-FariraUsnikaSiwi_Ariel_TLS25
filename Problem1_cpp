#include <iostream>
using namespace std;

// Fungsi untuk membalik angka secara matematis
int reverseNumber(int num) {
    int rev = 0, sign = 1;
    if (num < 0) {       // kalau negatif, simpan tanda
        sign = -1;
        num = -num;
    }
    while (num > 0) {
        rev = rev * 10 + num % 10; // ambil digit terakhir
        num /= 10;                 // buang digit terakhir
    }
    return rev * sign; // kembalikan tanda asli
}

int main() {
    int n;
    cout << "Masukkan jumlah elemen array: ";
    cin >> n;

    int arr[n];
    cout << "Masukkan elemen array:\n";
    for (int i = 0; i < n; i++) cin >> arr[i];

    cout << "Hasil inverse sequence:\n";
    for (int i = 0; i < n; i++) {
        cout << reverseNumber(arr[i]) << " ";
    }
    cout << endl;

    return 0;
}
