#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Masukkan jumlah detik simulasi: ";
    cin >> n;

    // Array warna lampu
    string lights[3] = {"Hijau", "Kuning", "Merah"};

    // Loop simulasi
    for (int i = 0; i < n; i++) {
        cout << "Detik " << i + 1 << ": " << lights[i % 3] << endl;
    }

    return 0;
}
